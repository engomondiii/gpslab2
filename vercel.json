{
  "version": 2,
  "builds": [
    {
      "src": "gpslab2/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "100mb",
        "runtime": "python3.12"
      }
    }
  ],
  "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "gpslab2/wsgi.py" }
  ]
}
